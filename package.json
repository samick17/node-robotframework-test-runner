{
  "name": "node-robotframework-test-runner",
  "version": "1.0.1",
  "description": "Robotframework for nodejs",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "start": "node ./test-runner.js",
    "runtest": "node ./index.js -test",
    "robot": "robot ./testcases/test.txt",
    "test": "node_modules/.bin/mocha ./test/**/*.mocha.js",
    "server": "node ./core/lib.js -lib \"\"\"\"../lib.js\"\"\"\"",
    "clean": "node ./core/misc/cleanup.js",
    "gentrendreport": "node ./index.js -genstatistics"
  },
  "keywords": [
    "RobotFramework",
    "Testing",
    "Keyword Driven",
    "TestAutomation"
  ],
  "author": {
    "name": "Samick",
    "email": "boneache@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.1",
    "clipboardy": "^1.2.3",
    "robotremote": "^1.3.0",
    "xmlrpc": "^1.3.2"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.1.1",
    "should": "^13.2.1"
  }
}
